{
  "expo": {
    "name": "ClearSkin AI",
    "slug": "clearskin-ai",
    "scheme": "clearskinai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": { "image": "./assets/splash-icon.png", "resizeMode": "contain", "backgroundColor": "#ffffff" },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.clearskin.ai",
      "infoPlist": {
        "NSCameraUsageDescription": "We use the camera to capture your face photos for skin analysis."
      },
      "associatedDomains": ["applinks:clearskinai.com"],
      "merchantId": "merchant.com.clearskin.ai"
    },
    "android": {
      "permissions": ["CAMERA"],
      "package": "com.clearskin.ai"
    },
    "plugins": ["expo-router"],
    "experiments": { "typedRoutes": true }
  }
}
